<view class="flexRow">
  <view class="flexStart">
    <van-icon name="clock" color="#00AA00" />
    <text style="color:#00AA00;font-size:small;margin-left:15rpx">个人信息</text>
  </view>
  <!-- <view>
    <button size="mini" type="primary" plain="true">修改</button>
  </view> -->
</view>

<view>
  <van-cell-group custom-class="margin2">
    <van-cell>
      <view class="flexCell">
        <view>姓名</view>
        <view class="textblack">{{name}}</view>
      </view>
      <view class="flexCell">
        <view>性别</view>
        <view class="textblack" space="emsp">{{gender}}</view>
      </view>
      <view class="flexCell">
        <view>年龄</view>
        <view class="textblack" space="emsp">{{age}}</view>
      </view>
      <view class="flexCell">
        <view>联络电话</view>
        <view class="textblack">{{phoneNum}}</view>
      </view>
      <view class="flexCell">
        <view>位置</view>
        <view class="textblack">{{location}}</view>
      </view>
    </van-cell>

  </van-cell-group>
</view>

<van-divider contentPosition="center">已经到底啦</van-divider>