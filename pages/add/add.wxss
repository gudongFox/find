/* pages/add/add.wxss */
.whitebackground{
  background-color: white !important;
}
.appading{
  padding-top: 1em;
  margin-left: 1rem;
}
.appadingleft{
  margin-left: 1rem;
}

.appading1{
  padding-top: 0.5em;
  padding-bottom: 0.5rem;
}
.member{
  width: 250rpx;
  background: #fff;
  padding: 20rpx;
  border-radius: 20rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  align-items: center;
}

.goods{
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  align-items: center;
}
.goods:nth-child(2n) {
  margin-right: 0;
}
/* pages/newDemandServer/newDemandServer.wxss */
.specialLine{
  width: 750rpx;
  height: 15rpx;
}

.specialBlock {
  width: 15rpx;
  height: 10rpx;
}

.flexStart {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
}

.flexRow {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  margin-top: 0.5em;
  margin-left: 0.5rem;
  margin-bottom: 0.5rem;
  margin-right: 1rem;
}

.flexCell {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.flexCol {
  display: flex;
  flex-direction: column;
}
.tagDisplay{
  padding-bottom: 10px;
}