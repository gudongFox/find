<!--pages/manage/manage.wxml-->
<view>
<van-tabs active="{{ active }}" color="#00AA00" class="tabw">
		<van-tab title="客户新需求">
      <view>
				
				<view class="specialLine"></view>
        <view class="flexStart">
          <view class="specialBlock"></view>
          <van-icon name="clock" color="#00AA00" />
          <text style="color:#00AA00;font-size:small;margin-left:15rpx">待处理的客户需求</text>
        </view>
        <view class="specialLine"></view>
				
				<view>
          <van-cell-group>
            <van-cell clickable="true">
              <view class="flexRow">
								<view class="flexCol">
									<van-icon name="wap-home-o" size="50rpx" color="#66BB6A"></van-icon>
									<text style="font-size:10;color:#66BB6A">家庭保洁</text>
								</view>
								<view class="flexCol" style="align-items:flex-start">
									<view class="flexRow">
										<text class="textbig,textblack">马先生</text>
										<view class="specialBlock"></view>
										<text>上门时间：未定</text>
										<view class="specialBlock"></view>
										<van-tag round type="primary">多次</van-tag>
									</view>
									<text>虎溪花园，4栋3-3-1</text>
								</view>
								<van-tag plain type="warning" size="medium">待处理</van-tag>
							</view>
            </van-cell>
          </van-cell-group>
        </view>
				
				<view class="divLine"></view>
				
				<view>
          <van-cell-group>
            <van-cell clickable="true">
              <view class="flexRow">
								<view class="flexCol">
									<van-icon name="wap-home-o" size="50rpx" color="#66BB6A"></van-icon>
									<text style="font-size:10;color:#66BB6A">家庭保洁</text>
								</view>
								<view class="flexCol" style="align-items:flex-start">
									<view class="flexRow">
										<text class="textbig,textblack">马先生</text>
										<view class="specialBlock"></view>
										<text>上门时间：未定</text>
										<view class="specialBlock"></view>
										<van-tag round type="primary">多次</van-tag>
									</view>
									<text>虎溪花园，4栋3-3-1</text>
								</view>
								<van-tag plain type="warning" size="medium">待处理</van-tag>
							</view>
            </van-cell>
          </van-cell-group>
        </view>
      </view>
		</van-tab>

		<van-tab title="代客户下单">
      <view>
				
				<view class="specialLine"></view>
        <view class="flexStart">
          <view class="specialBlock"></view>
          <van-icon name="clock" color="#00AA00" />
          <text style="color:#00AA00;font-size:small;margin-left:15rpx">我的直接客户</text>
        </view>
        <view class="specialLine"></view>
				
				<view>
          <van-cell-group>
            <van-cell clickable="true">
              <view class="flexRow">
                <van-image round width="50" height="50" src="https://img.yzcdn.cn/vant/cat.jpeg" />
                <view style="display:flex;flex-direction:column">
                  <view class="flexRow">
                    <text class="textbig,textblack">王阿姨</text>
                    <van-tag type="primary" plain>家庭保洁</van-tag>
                  </view>
                  <text class="textsmall">成都市锦兴路锦江文化中心大门</text>
                </view>
                <view style="display:flex;flex-direction:column">
                  <van-tag mark>服务(13)</van-tag>
                  <van-tag mark>委托(5)</van-tag>
                </view>
              </view>
            </van-cell>
          </van-cell-group>
        </view>

        <view class="divLine"></view>

        <view>
          <van-cell-group>
            <van-cell clickable="true">
              <view class="flexRow">
                <van-image round width="50" height="50" src="https://img.yzcdn.cn/vant/cat.jpeg" />
                <view style="display:flex;flex-direction:column">
                  <view class="flexRow">
                    <text class="textbig,textblack">何老师</text>
                    <van-tag type="primary" plain>钟点工</van-tag>
                  </view>
                  <text class="textsmall">成都市锦兴路锦江文化中心大门</text>
                </view>
                <view style="display:flex;flex-direction:column">
                  <van-tag mark>服务(3)</van-tag>
                  <van-tag mark>委托(5)</van-tag>
                </view>
              </view>
            </van-cell>
          </van-cell-group>
				</view>
				

      </view>
		</van-tab>

		<van-tab disabled></van-tab>

		<van-tab title="设置">
			
			<view class="specialLine"></view>
        <view class="flexRow">
          <view class="flexStart">
            <view class="specialBlock"></view>
            <van-icon name="clock" color="#00AA00" />
            <text style="color:#00AA00;font-size:small;margin-left:15rpx">可接单时段</text>
          </view>
          <view>
						<button size="mini" type="primary" plain="true">修改</button>
          </view>
        </view>
        <view class="specialLine"></view>
				
