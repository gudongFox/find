				<view class="specialLine"></view>
        <view class="flexRow">
          <view class="flexStart">
            <view class="specialBlock"></view>
            <van-icon name="clock" color="#00AA00" />
            <text style="color:#00AA00;font-size:small;margin-left:15rpx">接单参数设置</text>
          </view>
          <!-- <view>
						<button size="mini" type="primary" plain="true">修改</button>
          </view> -->
        </view>
				<view class="specialLine"></view>
				
				<view class="flexCol">
					<view class="flexRow">
						<view><text>出发上门最大距离（公里）</text></view>
						<van-stepper value="{{ 1 }}" bind:change="onChange" />
					</view>
					<view class="flexRow">
						<view><text>两单间隔最大距离（公里）</text></view>
						<van-stepper value="{{ 1 }}" bind:change="onChange" />
					</view>
					<view class="flexRow">
						<view><text>最小服务时长（小时）</text></view>
						<van-stepper value="{{ 1 }}" step="0.5" decimal-length="{{1}}" />
					</view>
					<view class="flexRow">
						<view><text>两单间隔最大时间</text></view>
						<van-stepper value="{{ 1 }}" step="0.5" decimal-length="{{1}}" />
					</view>
				</view>
				

		</van-tab>
</van-tabs>
</view>